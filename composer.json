{
    "name": "judopay/judopay-sdk",
    "type": "library",
    "description": "SDK for the Judopay API",
    "keywords": [
        "payment"
    ],
    "homepage": "https://www.judopay.com/docs",
    "license": "MIT",
    "authors": [
        {
            "name": "Judopay",
            "email": "developersupport@judopay.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "pimple/pimple": "^3.0",
        "psr/log": "^1.0",
        "ext-openssl": ">=0.0.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.0",
        "squizlabs/php_codesniffer": "^4.0",
        "phpunit/phpunit": "^9.5",
        "symfony/finder": "^5.0",
        "symfony/console": "^5.0",
        "symfony/process": "^5.0",
        "symfony/yaml": "^5.0",
        "phpdocumentor/reflection-docblock": "^5.0",
        "friends-of-phpspec/phpspec-expect": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Judopay\\": "src/Judopay"
        },
        "files": [
            "src/Judopay.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "spec\\": "spec/"
        }
    }
}
